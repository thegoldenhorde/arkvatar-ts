{
  "name": "arkvatar-ts",
  "version": "1.0.0",
  "description": "Typescript package for interacting with the Arkvatar API.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "yarn run build",
    "prepublishOnly": "yarn test && yarn run lint",
    "preversion": "yarn run lint",
    "version": "yarn run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": "git+https://github.com/thegoldenhorde/arkvatar-ts.git",
  "keywords": [
    "Wrapper",
    "API",
    "Arkvatar"
  ],
  "author": "Highjhacker",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thegoldenhorde/arkvatar-ts/issues"
  },
  "homepage": "https://github.com/thegoldenhorde/arkvatar-ts#readme",
  "devDependencies": {
    "@types/jest": "^24.0.17",
    "@types/node": "^12.7.1",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "@types/got": "^9.6.5",
    "got": "^9.6.0"
  }
}
