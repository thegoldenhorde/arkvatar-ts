{
  "name": "arkvatar-ts",
  "version": "1.0.0",
  "description": "Typescript package for interacting with the Arkvatar API.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thegoldenhorde/arkvatar-ts.git"
  },
  "keywords": [
    "Wrapper",
    "API",
    "Arkvatar"
  ],
  "author": "Highjhacker",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thegoldenhorde/arkvatar-ts/issues"
  },
  "homepage": "https://github.com/thegoldenhorde/arkvatar-ts#readme",
  "devDependencies": {
    "@types/jest": "^24.0.17",
    "@types/node": "^12.7.1",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "got": "^9.6.0",
    "nock": "^10.0.6"
  }
}
